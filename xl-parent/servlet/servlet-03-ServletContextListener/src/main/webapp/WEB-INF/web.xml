<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <!--web.xml中的文件是又顺序的不能乱排序，发现把listener放servlet后就文件报错了-->
  <listener>
    <listener-class>com.xl.servlet03servletcontextlistener.listener.MyListener</listener-class>
  </listener>

  <servlet>
  <servlet-name>MyServlet02</servlet-name>
  <servlet-class>com.xl.servlet03servletcontextlistener.MyServlet02</servlet-class>
</servlet>
  <servlet-mapping>
    <servlet-name>MyServlet02</servlet-name>
    <url-pattern>/my2</url-pattern>
  </servlet-mapping>



</web-app>
